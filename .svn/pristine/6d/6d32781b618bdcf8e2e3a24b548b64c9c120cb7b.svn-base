#import <UIKit/UIKit.h>

/**统一的脚视图，包含“暂无更多数据”和“正在加载更多”提示，适用于自动上拉刷新表格*/
@interface FooterInfoView : UIView

/**⭐️视图切换方法，输入"YES"显示@"暂无更多数据"，输入"NO"显示@"暂无更多数据"*/
- (void)isDataCompleted:(BOOL)isCompleted;

@end
