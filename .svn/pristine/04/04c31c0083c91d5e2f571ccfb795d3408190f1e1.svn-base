<widget xmlns     = "http://www.w3.org/ns/widgets"
        id        = "io.cordova.helloCordova"
        version   = "2.0.0">
    <name>Hello Cordova</name>

    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>

    <author href="http://cordova.io" email="dev@cordova.apache.org">
        Apache Cordova Team
    </author>

    <access origin="*"/>

    <!-- <content src="http://mysite.com/myapp.html" /> for external pages -->
    <content src="m/index.html" />

    <!-- Preferences for iOS -->
    <preference name="AllowInlineMediaPlayback" value="false" />
    <preference name="AutoHideSplashScreen" value="true" />
    <preference name="BackupWebStorage" value="cloud" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="EnableLocation" value="false" /><!-- DEPRECATED -->
    <preference name="EnableViewportScale" value="false" />
    <preference name="FadeSplashScreen" value="true" />
    <preference name="FadeSplashScreenDuration" value=".25" />
    <preference name="HideKeyboardFormAccessoryBar" value="false" />
    <preference name="KeyboardDisplayRequiresUserAction" value="true" />
    <preference name="KeyboardShrinksView" value="false" />
    <preference name="MediaPlaybackRequiresUserAction" value="false" />
    <preference name="ShowSplashScreenSpinner" value="true" />
    <preference name="SuppressesIncrementalRendering" value="false" />
    <preference name="TopActivityIndicator" value="gray" />



    <feature name="Geolocation">
      <param name="ios-package" value="CDVLocation"/>
    </feature>
    <feature name="Device">
      <param name="ios-package" value="CDVDevice"/>
    </feature>
    <feature name="Accelerometer">
      <param name="ios-package" value="CDVAccelerometer"/>
    </feature>
    <feature name="Compass">
      <param name="ios-package" value="CDVLocation"/>
    </feature>
    <feature name="Media">
      <param name="ios-package" value="CDVSound"/>
    </feature>
    <feature name="Camera">
      <param name="ios-package" value="CDVCamera"/>
    </feature>
    <feature name="Contacts">
      <param name="ios-package" value="CDVContacts"/>
    </feature>
    <feature name="File">
      <param name="ios-package"  value="CDVFile"/>
    </feature>
    <feature name="NetworkStatus">
      <param name="ios-package" value="CDVConnection"/>
    </feature>
    <feature name="Notification">
      <param name="ios-package" value="CDVNotification"/>
    </feature>
    <feature name="FileTransfer">
      <param name="ios-package" value="CDVFileTransfer"/>
    </feature>
    <feature name="Capture">
      <param name="ios-package" value="CDVCapture"/>
    </feature>
    <feature name="Battery">
      <param name="ios-package" value="CDVBattery"/>
    </feature>
    <feature name="SplashScreen">
      <param name="ios-package" value="CDVSplashScreen"/>
    </feature>
    <feature name="Echo">
      <param name="ios-package" value="CDVEcho"/>
    </feature>
    <feature name="Globalization">
      <param name="ios-package" value="CDVGlobalization"/>
    </feature>
    <feature name="InAppBrowser">
      <param name="ios-package" value="CDVInAppBrowser"/>
    </feature>
    <feature name="Logger">
      <param name="ios-package" value="CDVLogger"/>
    </feature>
    <feature name="LocalStorage">
        <param name="ios-package" value="CDVLocalStorage"/>
    </feature>

    <feature name="carmeraPlugin">
        <param name="ios-package" value="CarmeraPlugin" />
    </feature>
    
    <feature name="clearZsPlugin">
        <param name="ios-package" value="ClearZsPlugin" />
    </feature>
    
    <feature name="closeAppPlugin">
        <param name="ios-package" value="CloseAppPlugin" />
    </feature>
    
    <feature name="createKeyPlugin">
        <param name="ios-package" value="CreateKeyPlugin" />
    </feature>
    
    <feature name="fileIsExistsPlugin">
        <param name="ios-package" value="FileIsExistsPlugin" />
    </feature>
    
    <feature name="getVersionPlugin">
        <param name="ios-package" value="GetVersionPlugin" />
    </feature>
    
    <feature name="initZsPlugin">
        <param name="ios-package" value="InitZsPlugin" />
    </feature>
    
    <feature name="logicPlugin">
        <param name="ios-package" value="LogicPlugin" />
    </feature>
    
    <feature name="signPlugin">
        <param name="ios-package" value="SignPlugin" />
    </feature>
    
    <feature name="videoWitnessPlugin">
        <param name="ios-package" value="VideoWitnessPlugin" />
    </feature>
    
    <feature name="waitDialogPlugin">
        <param name="ios-package" value="WaitDialogPlugin" />
    </feature>
    
    <feature name="videoDialogPlugin">
        <param name="ios-package" value="VideoDialogPlugin" />
    </feature>
    
    <feature name="toastPlugin">
        <param name="ios-package" value="ToastPlugin" />
    </feature>
    
    <feature name="pwdDesPlugin">
        <param name="ios-package" value="PwdDesPlugin" />
    </feature>
    
    <feature name="updateManagerPlugin">
        <param name="ios-package" value="UpdateManagerPlugin" />
    </feature>
    
    <feature name="webViewPlugin">
        <param name="ios-package" value="WebViewPlugin" />
    </feature>
    
    <feature name="isNetAvailablePlugin">
        <param name="ios-package" value="IsNetAvaliablePlugin" />
    </feature>
    
    <feature name="calendarPlugin">
        <param name="ios-package" value="CalendarPlugin" />
    </feature>
    
    <feature name="getIpMacPLugin">
        <param name="ios-package" value="GetMacPlugin" />
    </feature>
    
    <feature name="speedServerPlugin">
        <param name="ios-package" value="SpeedServerPlugin" />
    </feature>
    <feature name="softKeyboardPlugin">
        <param name="ios-package" value="SoftKeyboardPlugin" />
    </feature>
    
    <feature name="getLocationInfoPlugin">
        <param name="ios-package" value="GetLocationInfoPlugin" />
    </feature>
    
    <feature name="getChannelPlugin">
        <param name="ios-package" value="GetChannelPlugin"/>
    </feature>
    
    <feature name="setLockScreenPlugin">
        <param name="ios-package" value="SetLockScreenPlugin"/>
    </feature>
    
    <feature name="unLockScreenPlugin">
        <param name="ios-package" value="UnLockScreenPlugin"/>
    </feature>
    
    <feature name="clearPassPlugin">
        <param name="ios-package" value="ClearPassPlugin"/>
    </feature>
    
    <feature name="getNetworkStatusPlugin">
        <param name="ios-package" value="GetNetworkStatusPlugin"/>
    </feature>
    
    <feature name="callPhonePlugin">
        <param name="ios-package" value="CallPhonePlugin"/>
    </feature>
    
    
    <feature name="fileWriteReadPlugin">
        <param name="ios-package" value="FileWriteReadPlugin"/>
    </feature>
    
    <feature name="sharePlugin">
        <param name="ios-package" value="SharePlugin"/>
    </feature>

    <!-- Deprecated plugins element. REmove in 3.0 -->
    <plugins>
    </plugins>
</widget>
