  NSString *UserID = [SimuUtil getUserID];
  NSString *Key = [NSString stringWithFormat:@"YLDistributeVC_%@", UserID];
  [[NSUserDefaults standardUserDefaults] setObject:dic forKey:Key];
  [super leftButtonPress];
}

